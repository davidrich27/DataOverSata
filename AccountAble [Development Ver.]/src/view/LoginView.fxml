<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane nodeOrientation="LEFT_TO_RIGHT" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Button fx:id="loginBtn" layoutX="115.0" layoutY="230.0" mnemonicParsing="false" onAction="#handleLogin" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <PasswordField fx:id="pwdTxt" layoutX="39.0" layoutY="141.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <TextField fx:id="usernameTxt" layoutX="39.0" layoutY="96.0" promptText="Username">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Label fx:id="warningLbl" alignment="CENTER" layoutX="3.0" layoutY="189.0" prefHeight="17.0" prefWidth="294.0" text="Status" textFill="#e80a0a">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="lblStatus1" alignment="CENTER" layoutX="3.0" layoutY="61.0" prefHeight="17.0" prefWidth="294.0" text="Please Log In">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="lblStatus11" alignment="CENTER" layoutX="3.0" layoutY="21.0" prefHeight="17.0" prefWidth="294.0" text="AccountAble">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
