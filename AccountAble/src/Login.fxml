         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <PasswordField fx:id="txtPassword" layoutX="39.0" layoutY="141.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <TextField fx:id="txtUsername" layoutX="38.0" layoutY="65.0" promptText="Username">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Label fx:id="lblStatus" alignment="CENTER" layoutX="4.0" layoutY="27.0" prefHeight="17.0" prefWidth="294.0" text="Status" textFill="#e80a0a">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
