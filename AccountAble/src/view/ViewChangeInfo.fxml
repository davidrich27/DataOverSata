<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ChangeInfoController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="161.0" layoutY="63.0" text="Username:" />
            <Label layoutX="162.0" layoutY="96.0" text="Password:" />
            <Label layoutX="159.0" layoutY="130.0" text="Confirm Password:" />
            <Label layoutX="159.0" layoutY="162.0" text="First Name:" />
            <Label layoutX="159.0" layoutY="193.0" text="Last Name:" />
            <Label layoutX="161.0" layoutY="223.0" text="Email:" />
            <Label layoutX="161.0" layoutY="256.0" text="Phone Number:" />
            <TextField fx:id="usernameTxt" layoutX="287.0" layoutY="57.0" />
            <TextField fx:id="newPwdTxt1" layoutX="287.0" layoutY="90.0" />
            <TextField fx:id="newPwdTxt2" layoutX="287.0" layoutY="124.0" />
            <TextField fx:id="firstnameTxt" layoutX="287.0" layoutY="156.0" />
            <TextField fx:id="lastnameTxt" layoutX="287.0" layoutY="187.0" />
            <TextField fx:id="emailTxt" layoutX="287.0" layoutY="217.0" />
            <TextField fx:id="phoneTxt" layoutX="287.0" layoutY="250.0" />
            <TextField fx:id="oldPwdTxt" layoutX="287.0" layoutY="283.0" />
            <Label layoutX="157.0" layoutY="289.0" text="Enter Old Password:" />
            <Button fx:id="confirmBtn" layoutX="224.0" layoutY="359.0" mnemonicParsing="false" onAction="#confirmClick" text="Confirm" />
            <Button fx:id="cancelBtn" layoutX="299.0" layoutY="359.0" mnemonicParsing="false" onAction="#cancelClick" text="Cancel" />
            <Label fx:id="titleLbl" layoutX="184.0" layoutY="15.0" text="Edit User Account Details">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="warningLbl" layoutX="185.0" layoutY="325.0" text="Sorry, that is the wrong password." textFill="RED">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
